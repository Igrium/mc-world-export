{
  "schemaVersion": 1,
  "id": "worldexport",
  "version": "${version}",

  "name": "Igrium's Replay Exporter",
  "description": "An addon to the Replay Mod that allows it to export into Blender and other 3D software.",
  "authors": [
    "Igrium"
  ],
  "contact": {
    "homepage": "https://github.com/Sam54123/mc-world-export",
    "sources": "https://github.com/Sam54123/mc-world-export"
  },

  "license": "GPL-3.0-or-later",
  "icon": "assets/worldexport/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
    ],
    "client": [
      "org.scaffoldeditor.worldexport.ReplayExportMod"
    ]
  },
  "mixins": [
    "worldexport.mixins.json",
    "debugger.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17",
    "replaymod": ">=1.19.3-2.6.10"
  },
  "suggests": {
    "another-mod": "*"
  }
}
